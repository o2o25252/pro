<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="chart">

	<!-- 전체 게시물 수 -->
	<select id="chkChartData" resultType="com.data.vo.ChartVO" parameterType="java.util.Map">
		select * from chartData_t
		<![CDATA[
		where cDate >= to_date(#{tarDay}, 'YYMMDD')
		and cDate < to_date(#{strDate}, 'YYMMDD')
		]]>
	</select> 
	
</mapper>